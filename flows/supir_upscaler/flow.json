{
  "name": "supir_upscaler",
  "display_name": "SUPIR Upscaler",
  "description": "High-End Image Upscaler 1.1x-4x",
  "author": "akihungac",
  "homepage": "https://openart.ai/workflows/profile/akihungac",
  "license": "",
  "documentation": "https://visionatrix.github.io/Visionatrix/Flows/SupirUpscaler.html",
  "sub_flows": [
    {
      "display_name": "SUPIR(1.5x)",
      "type": "image",
      "input_params": [
        {
          "name": "upscale_at_end",
          "default": false
        }
      ]
    },
    {
      "display_name": "SUPIR(2x)",
      "type": "image",
      "input_params": [
        {
          "name": "upscale_at_end",
          "default": true
        }
      ]
    }
  ],
  "input_params": [
    {
      "name": "image",
      "display_name": "Image to upscale",
      "type": "image",
      "optional": false,
      "comfy_node_id": {
        "2": {
          "dest_field_name": ["inputs", "image"]
        }
      }
    },
    {
      "name": "result_resolution",
      "display_name": "Output image size factor",
      "type": "range_scale",
      "source_input_name": "image",
      "optional": true,
      "advanced": true,
      "default": 2.0,
      "min": 1.1,
      "max": 4.0,
      "step": 0.1,
      "comfy_node_id": {
        "104": {
          "dest_field_name": ["inputs", "scale_by"],
          "internal_type": "float"
        }
      }
    },
    {
      "name": "supir_diff_type",
      "display_name": "Diffusion Type(set to \"fp32\" on macOS)",
      "type": "list",
      "default": "auto",
      "optional": true,
      "advanced": true,
      "options": {
        "fp16": "fp16",
        "bf16": "bf16",
        "fp32": "fp32",
        "auto": "auto"
      },
      "comfy_node_id": {
        "21": {
          "dest_field_name": ["inputs", "diffusion_dtype"]
        }
      }
    },
    {
      "name": "sampler",
      "display_name": "Sharpening",
      "type": "bool",
      "default": true,
      "optional": true,
      "advanced": true,
      "comfy_node_id": {
        "7": {
          "dest_field_name": ["inputs", "sampler"],
          "value": "TiledRestoreDPMPP2MSampler"
        }
      }
    },
    {
      "name": "low_resource",
      "display_name": "Low resource mode",
      "type": "bool",
      "default": false,
      "optional": true,
      "advanced": true,
      "comfy_node_id": {
        "21": {
          "dest_field_name": ["inputs", "fp8_unet"],
          "value": true
        },
        "7": {
          "dest_field_name": ["inputs", ["sampler_tile_size", "sampler_tile_stride"]],
          "value": [512, 256]
        },
        "5": {
          "dest_field_name": ["inputs", ["decoder_tile_size", "encoder_tile_size"]],
          "value": [512, 512]
        },
        "10": {
          "dest_field_name": ["inputs", "decoder_tile_size"],
          "value": 512
        },
        "11": {
          "dest_field_name": ["inputs", "encoder_tile_size"],
          "value": 512
        }
      }
    }
  ]
}
