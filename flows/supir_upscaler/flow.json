{
  "name": "supir_upscaler",
  "display_name": "SUPIR Upscaler",
  "description": "High-End Image Upscaler 2x-4x",
  "author": "akihungac",
  "homepage": "https://openart.ai/workflows/profile/akihungac",
  "license": "",
  "documentation": "https://visionatrix.github.io/Visionatrix/Flows/SupirUpscaler.html",
  "sub_flows": [
    {
      "display_name": "SUPIR(2x)",
      "type": "image",
      "input_params": [
        {
          "name": "upscale_at_end",
          "default": false
        }
      ]
    },
    {
      "display_name": "SUPIR(4x)",
      "type": "image",
      "input_params": [
        {
          "name": "upscale_at_end",
          "default": true
        }
      ]
    }
  ],
  "input_params": [
    {
      "name": "image",
      "display_name": "Low resolution image",
      "type": "image",
      "optional": false,
      "comfy_node_id": {
        "2": {
          "dest_field_name": ["inputs", "image"]
        }
      }
    },
    {
      "name": "upscale_at_end",
      "display_name": "Perform additional 2x upscale after SUPIR",
      "type": "bool",
      "default": false,
      "optional": true,
      "advanced": true,
      "comfy_node_id": {
        "84": {
          "dest_field_name": ["inputs", "images"],
          "value": ["42", 0]
        }
      }
    },
    {
      "name": "downscale_input_to",
      "display_name": "If input size is larger, scale down to",
      "type": "range",
      "optional": true,
      "advanced": true,
      "default": 832,
      "min": 512,
      "max": 2048,
      "step": 32,
      "comfy_node_id": {
        "83": {
          "dest_field_name": ["inputs", ["width", "height"]],
          "internal_type": "int"
        }
      }
    },
    {
      "name": "supir_diff_type",
      "display_name": "Diffusion Type(set to \"fp32\" on macOS)",
      "type": "list",
      "default": "auto",
      "optional": true,
      "advanced": true,
      "options": {
        "fp16": "fp16",
        "bf16": "bf16",
        "fp32": "fp32",
        "auto": "auto"
      },
      "comfy_node_id": {
        "21": {
          "dest_field_name": ["inputs", "diffusion_dtype"]
        }
      }
    },
    {
      "name": "low_resource",
      "display_name": "Low resource mode(fp8_unet)",
      "type": "bool",
      "default": false,
      "optional": true,
      "advanced": true,
      "comfy_node_id": {
        "21": {
          "dest_field_name": ["inputs", "fp8_unet"],
          "value": true
        }
      }
    }
  ]
}
